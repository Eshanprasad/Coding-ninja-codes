# my approach 1
li = [0, 2, 3, 9, 23, 34, 49, 53, 65, 69, 74]
target = int(input("Enter a number to be found: "))
start = 0
end = len(li)-1
while start<=end:
    mid = (start+end)//2
    if li[mid]==target:
        print("index of the number is:",mid)
        break
    elif target<li[mid]:
        end = mid-1
    elif li[mid]<target:
        start = mid+1
    else:
        print("number is not in the list")
else:
    print("not found")
