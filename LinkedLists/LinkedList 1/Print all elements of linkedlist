class Node:
    def __init__(self, data):
        self.data = data
        self.next = None
    @staticmethod
    def printLinkedList(head):
        while head is not None:
            print(head.data, end= " ")
            head = head.next
a = Node(10)
b = Node(22)
c = Node(43)
d = Node(87)
a.next = b
b.next = c
c.next = d
Node.printLinkedList(a)
